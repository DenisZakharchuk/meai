{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "ensure-docker-compose",
            "program": "${workspaceFolder}/bin/Debug/net8.0/MeAI.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "env": {},
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": ".NET Core Launch (Release)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "ensure-docker-compose",
            "program": "${workspaceFolder}/bin/Release/net8.0/MeAI.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "env": {},
            "internalConsoleOptions": "neverOpen"
        }
    ]
}